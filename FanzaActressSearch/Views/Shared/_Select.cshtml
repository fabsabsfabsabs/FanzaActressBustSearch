@model FanzaActressSearch.ViewModel.Select.IViewSelect
<p class="p-1">@Model.Title:</p>
<select id="@Model.Id" class="form-control form-control-sm" onchange="reloadWithoutPage('@(Model.Id)')">
    @{
        foreach (var item in Model.Items)
        {
            <option value=@(item.Key) selected="@(Model.Value == item.Key)">@item.Value</option>
        }
    }
</select>
